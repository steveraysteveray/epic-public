.
quantity:GeneralizedCoordinate
  rdf:type qudt:MechanicsQuantityKind ;
  rdf:type qudt:QuantityKind ;
  qudt:applicableUnit unit:UNITLESS ;
  qudt:description "Generalized Coordinates refers to the parameters that describe the configuration of the system relative to some reference configuration. These parameters must uniquely define the configuration of the system relative to the reference configuration."^^xsd:string ;
  qudt:latexDefinition "$q_i$, where $q_i$ is one of the coordinates that is used to describe the position of the system under consideration, and $N$ is the lowest number of coordinates necessary to fully define the position of the system."^^xsd:string ;
  qudt:symbol "q_i"^^xsd:string ;
  rdfs:label "Generalized Coordinate"^^xsd:string ;
  skos:prefLabel "Generalized Coordinate"^^xsd:string ;
  prov:wasDerivedFrom <http://www.iso.org/iso/catalogue_detail?csnumber=31889> ;
  prov:wasInfluencedBy <http://en.wikipedia.org/wiki/Generalized_coordinates> ;
.
quantity:GeneralizedForce
  rdf:type qudt:MechanicsQuantityKind ;
  rdf:type qudt:QuantityKind ;
  qudt:applicableUnit unit:UNITLESS ;
  qudt:description "Generalized Forces find use in Lagrangian mechanics, where they play a role conjugate to generalized coordinates."^^xsd:string ;
  qudt:latexDefinition "$\\delta A = \\sum Q_i\\delta q_i$, where $A$ is work and $q_i$ is a generalized coordinate."^^xsd:string ;
  qudt:symbol "Q_i"^^xsd:string ;
  rdfs:label "Generalized Force"^^xsd:string ;
  skos:prefLabel "Generalized Force"^^xsd:string ;
  prov:wasDerivedFrom <http://www.iso.org/iso/catalogue_detail?csnumber=31889> ;
  prov:wasInfluencedBy <http://en.wikipedia.org/wiki/Generalized_forces> ;
.
quantity:GeneralizedMomentum
  rdf:type qudt:MechanicsQuantityKind ;
  rdf:type qudt:QuantityKind ;
  qudt:applicableUnit unit:UNITLESS ;
  qudt:description "Generalized Momentum, also known as the canonical or conjugate momentum, extends the concepts of both linear momentum and angular momentum. To distinguish it from generalized momentum, the product of mass and velocity is also referred to as mechanical, kinetic or kinematic momentum."^^xsd:string ;
  qudt:latexDefinition "$p_i = \\frac{\\partial L}{\\partial \\dot{q_i}}$, where $L$ is the Langrange function and $\\dot{q_i}$ is a generalized velocity."^^xsd:string ;
  qudt:symbol "p_i"^^xsd:string ;
  rdfs:label "Generalized Force"^^xsd:string ;
  skos:prefLabel "Generalized Force"^^xsd:string ;
  prov:wasDerivedFrom <http://www.iso.org/iso/catalogue_detail?csnumber=31889> ;
  prov:wasInfluencedBy <http://en.wikipedia.org/wiki/Momentum> ;
.
quantity:GeneralizedVelocity
  rdf:type qudt:MechanicsQuantityKind ;
  rdf:type qudt:QuantityKind ;
  qudt:applicableUnit unit:UNITLESS ;
  qudt:description "Generalized Velocities are the time derivatives of the generalized coordinates of the system."^^xsd:string ;
  qudt:latexDefinition "$\\dot{q_i} = \\frac{dq_i}{dt}$, where $q_i$ is the generalized coordinate and $t$ is time."^^xsd:string ;
  qudt:symbol "\\dot{q_i}"^^xsd:string ;
  rdfs:label "Generalized Velocity"^^xsd:string ;
  skos:prefLabel "Generalized Velocity"^^xsd:string ;
  prov:wasDerivedFrom <http://www.iso.org/iso/catalogue_detail?csnumber=31889> ;
  prov:wasInfluencedBy <http://en.wikipedia.org/wiki/Generalized_coordinates> ;
.
quantity:HamiltonFunction
  rdf:type qudt:MechanicsQuantityKind ;
  rdf:type qudt:QuantityKind ;
  qudt:applicableUnit unit:JOULE ;
  qudt:description "The Hamilton–Jacobi equation (HJE) is a necessary condition describing extremal geometry in generalizations of problems from the calculus of variations."^^xsd:string ;
  qudt:latexDefinition "$H = \\sum p_i\\dot{q_i} - L$, where $p_i$ is a generalized momentum, $\\dot{q_i}$ is a generalized velocity, and $L$ is the Lagrange function."^^xsd:string ;
  qudt:symbol "H"^^xsd:string ;
  rdfs:label "Hamilton Function"^^xsd:string ;
  skos:prefLabel "Hamilton Function"^^xsd:string ;
  prov:wasDerivedFrom <http://www.iso.org/iso/catalogue_detail?csnumber=31889> ;
  prov:wasInfluencedBy <http://en.wikipedia.org/wiki/Hamilton–Jacobi_equation> ;
.
