quantity:Force
  rdf:type qudt:MechanicsQuantityKind ;
  qudt:applicableUnit unit:DYN ;
  qudt:applicableUnit unit:KGF ;
  qudt:applicableUnit unit:KIP ;
  qudt:applicableUnit unit:KiloPond ;
  qudt:applicableUnit unit:LBF ;
  qudt:applicableUnit unit:NEWTON ;
  qudt:applicableUnit unit:OZF ;
  qudt:applicableUnit unit:PDL ;
  qudt:applicableUnit unit:PlanckForce ;
  qudt:baseUnitDimensions "kg \\cdot m/s^2"^^xsd:string ;
  qudt:description "\\textit{Force} is an influence that causes mass to accelerate. It may be experienced as a lift, a push, or a pull. Force is defined by Newton's Second Law as $F = m \\times a $, where $F$ is force, $m$ is mass and $a$ is acceleration. Net force is mathematically equal to the time rate of change of the momentum of the body on which it acts. Since momentum is a vector quantity (has both a magnitude and direction), force also is a vector quantity."^^xsd:string ;
  qudt:symbol "F"^^xsd:string ;
  qudt:url "http://en.wikipedia.org/wiki/Force"^^xsd:anyURI ;
  rdfs:label "Force"^^xsd:string ;
  skos:exactMatch <http://dbpedia.org/resource/Force> ;
  skos:prefLabel "Force"^^xsd:string ;
  prov:wasInfluencedBy <http://en.wikipedia.org/wiki/Force> ;
.
