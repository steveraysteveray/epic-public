  qudt:description "A force is said to do Work when it acts on a body so that there is a displacement of the point of application, however small, in the direction of the force. "^^xsd:string ;
  qudt:latexDefinition "$A = \\int Pdt$, where $P$ is power and $t$ is time."^^xsd:string ;
  qudt:symbol "A"^^xsd:string ;
  rdfs:label "Work"^^xsd:string ;
  skos:prefLabel "Work"^^xsd:string ;
  prov:wasDerivedFrom <http://www.iso.org/iso/catalogue_detail?csnumber=31889> ;
  prov:wasInfluencedBy <http://en.wikipedia.org/wiki/Work_(physics)> ;
.
