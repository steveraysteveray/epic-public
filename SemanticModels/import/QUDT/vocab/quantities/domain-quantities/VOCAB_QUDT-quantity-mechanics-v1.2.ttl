quantity:Friction
  rdf:type qudt:MechanicsQuantityKind ;
  qudt:description "\\textit{Friction} is the force of two surfaces In contact, or the force of a medium acting on a moving object (that is air on an aircraft). When contacting surfaces move relative to each other, the friction between the two objects converts kinetic energy into thermal energy."^^xsd:string ;
  qudt:generalization quantity:Force ;
  qudt:symbol "\\mu"^^xsd:string ;
  qudt:url "http://wiki.answers.com/Q/What_is_the_symbol_of_friction"^^xsd:anyURI ;
