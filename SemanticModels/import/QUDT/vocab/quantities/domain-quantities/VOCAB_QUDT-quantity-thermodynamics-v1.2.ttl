quantity:Enthalpy
  rdf:type qudt:ThermodynamicsQuantityKind ;
  qudt:applicableUnit unit:JOULE ;
  qudt:description "In thermodynamics, \\textit{enthalpy} is the sum of the internal energy $U$ and the product of pressure $p$ and volume $V$ of a system. The characteristic function (also known as thermodynamic potential) \\textit{enthalpy} used to be called \\textit{heat contents}, which is why it is conventionally indicated by $H$. The specific enthalpy of a working mass is a property of that mass used in thermodynamics, defined as $h=u+p \\cdot v$,  where $u$ is the specific internal energy, $p$ is the pressure, and $v$ is specific volume. In other words, $h = H / m$ where $m$ is the mass of the system. The SI unit for \\textit{Specific Enthalpy} is \\textit{joules per kilogram}."^^xsd:string ;
  qudt:latexDefinition "$H = U + pV$, where $U$ is internal energy, $p$ is pressure and $V$ is volume."^^xsd:string ;
  qudt:symbol "H"^^xsd:string ;
  qudt:symbolToken "H"^^xsd:token ;
  rdfs:isDefinedBy <http://qudt.org/1.2/vocab/quantity/thermodynamics> ;
  rdfs:isDefinedBy <http://qudt.org/vocab/quantity/thermodynamics> ;
  rdfs:label "Enthalpy"^^xsd:string ;
  rdfs:seeAlso quantity:InternalEnergy ;
  skos:broader quantity:EnergyAndWork ;
  skos:exactMatch <http://dbpedia.org/resource/Enthalpy> ;
  skos:prefLabel "Enthalpy"^^xsd:string ;
  prov:wasDerivedFrom <http://www.iso.org/iso/catalogue_detail?csnumber=31890> ;
  prov:wasInfluencedBy <http://en.citizendium.org/wiki/Enthalpy> ;
  prov:wasInfluencedBy <http://en.wikipedia.org/wiki/Enthalpy> ;
.
quantity:Entropy
  rdf:type qudt:ThermodynamicsQuantityKind ;
  qudt:abbreviation "entropy"^^xsd:string ;
  qudt:applicableUnit unit:J-PER-K ;
  qudt:description "When a small amount of heat $dQ$ is received by a system whose thermodynamic temperature is $T$, the entropy of the system increases by $dQ/T$, provided that no irreversible change takes place in the system."^^xsd:string ;
  qudt:symbol "S"^^xsd:string ;
  qudt:symbolToken "S"^^xsd:token ;
  rdfs:isDefinedBy <http://qudt.org/1.2/vocab/quantity/thermodynamics> ;
