.
quantity:RecombinationCoefficient
  rdf:type qudt:AtomicPhysicsQuantityKind ;
  rdf:type qudt:QuantityKind ;
  qudt:applicableUnit unit:M3-PER-SEC ;
  qudt:description "The \\textit{Recombination Coefficient} is the rate of recombination of positive ions with electrons or negative ions in a gas, per unit volume, divided by the product of the number of positive ions per unit volume and the number of electrons or negative ions per unit volume."^^xsd:string ;
  qudt:latexDefinition "$-\\frac{dn^+}{dt} = -\\frac{dn^-}{dt} = an^+n^-$, where $n^+$ and $n^-$ are the ion number densities of positive and negative ions, respectively, recombined during an infinitesimal time interval with duration $dt$."^^xsd:string ;
  qudt:symbol "a"^^xsd:string ;
  rdfs:label "Recombination Coefficient"^^xsd:string ;
  skos:prefLabel "Recombination Coefficient"^^xsd:string ;
  prov:wasDerivedFrom <http://www.iso.org/iso/catalogue_detail?csnumber=31895> ;
  prov:wasInfluencedBy <http://encyclopedia2.thefreedictionary.com/recombination+coefficient> ;
.
quantity:ReducedPlanckConstant
  rdf:type qudt:AtomicPhysicsQuantityKind ;
  rdf:type qudt:PhysicalConstant ;
  qudt:applicableUnit unit:J-SEC ;
  qudt:description "The \\textit{Reduced Planck Constant}, or Dirac Constant, is used in applications where frequency is expressed in terms of radians per second (\\emph{angular frequency}) instead of cycles per second. In such cases a factor of $2\\pi$ is absorbed into the constant."^^xsd:string ;
  qudt:latexDefinition "$\\hbar = \\frac{h}{2\\pi}$, where $h$ is the Planck constant."^^xsd:string ;
  qudt:quantityValue quantity:Value_PlanckConstantOver2Pi ;
  qudt:symbol "\\hbar"^^xsd:string ;
  voag:hasGovernance quantity:CODATA_GovernanceDetails ;
  rdfs:label "Reduced Planck Constant"^^xsd:string ;
