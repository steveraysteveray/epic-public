quantity:AbsorbedDose
  rdf:type qudt:AtomicPhysicsQuantityKind ;
  qudt:applicableUnit unit:GRAY ;
  qudt:latexDefinition "$D = \\frac{d\\bar{\\varepsilon}}{dm}$, where $d\\bar{\\varepsilon}$ is the mean energy imparted by ionizing radiation to an element of irradiated matter with the mass $dm$."^^xsd:string ;
  qudt:symbol "D"^^xsd:string ;
  prov:wasDerivedFrom <http://www.iso.org/iso/catalogue_detail?csnumber=31895> ;
  prov:wasInfluencedBy <http://en.wikipedia.org/wiki/Absorbed_dose> ;
.
quantity:AbsorbedDoseRate
  rdf:type qudt:AtomicPhysicsQuantityKind ;
  qudt:applicableUnit unit:GY-PER-SEC ;
  qudt:latexDefinition "$\\dot{D} = \\frac{dD}{dt}$, where $dD$ is the increment of absorbed dose during time interval with duration $dt$."^^xsd:string ;
  qudt:symbol "\\dot{D}"^^xsd:string ;
  prov:wasDerivedFrom <http://www.iso.org/iso/catalogue_detail?csnumber=31895> ;
  prov:wasInfluencedBy <http://www.answers.com/topic/absorbed-dose-rate> ;
.
