quantity:RelativeMassExcess
  rdf:type qudt:AtomicPhysicsQuantityKind ;
  rdf:type qudt:QuantityKind ;
  qudt:applicableUnit unit:UNITLESS ;
  qudt:description "The \\textit{Relative Mass Excess} is the mass excess between the monoisotopic mass of an element and the mass of its A + 1 or its A + 2 isotopic cluster (extrapolated from relative mass defect)."^^xsd:string ;
  qudt:latexDefinition "$\\Delta_r = \\frac{\\Delta}{m_u}$, where $\\Delta$ is the mass excess and $m_u$ is the unified atomic mass constant."^^xsd:string ;
