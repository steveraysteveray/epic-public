.
quantity:EnergyFluenceRate
  rdf:type qudt:AtomicPhysicsQuantityKind ;
  rdf:type qudt:QuantityKind ;
  qudt:applicableUnit unit:W-PER-M2 ;
  qudt:description "\\textit{Energy Fluence Rate} can be used to describe the energy fluence delivered per unit time."^^xsd:string ;
  qudt:latexDefinition "$\\Psi = \\frac{d\\Psi}{dt}$, where $d\\Psi$ is the increment of the energy fluence during an infinitesimal time interval with duration $dt$."^^xsd:string ;
