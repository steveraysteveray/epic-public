.
quantity:SpinQuantumNumber
  rdf:type qudt:AtomicPhysicsQuantityKind ;
  rdf:type qudt:QuantityKind ;
  qudt:applicableUnit unit:UNITLESS ;
  qudt:description "The \\textit{Spin Quantum Number}  describes the spin (intrinsic angular momentum) of the electron within that orbital, and gives the projection of the spin angular momentum S along the specified axis"^^xsd:string ;
  qudt:latexDefinition "$s^2 = \\hbar^2 s(s + 1)$, where $s$ is the spin quantum number and $\\hbar$ is the Planck constant."^^xsd:string ;
  qudt:symbol "s"^^xsd:string ;
  rdfs:label "Spin Quantum Number"^^xsd:string ;
  skos:broader quantity:QuantumNumber ;
  skos:closeMatch quantity:MagneticQuantumNumber ;
  skos:closeMatch quantity:OrbitalAngularMomentumQuantumNumber ;
  skos:closeMatch quantity:PrincipalQuantumNumber ;
  skos:prefLabel "Spin Quantum Number"^^xsd:string ;
  prov:wasDerivedFrom <http://www.iso.org/iso/catalogue_detail?csnumber=31894> ;
  prov:wasInfluencedBy <http://en.wikipedia.org/wiki/Quantum_number> ;
.
quantity:SurfaceActivityDensity
  rdf:type qudt:AtomicPhysicsQuantityKind ;
  rdf:type qudt:QuantityKind ;
  qudt:applicableUnit unit:BQ-PER-M2 ;
  qudt:description "The \\textit{Surface Activity Density} is undefined."^^xsd:string ;
  qudt:latexDefinition "$a_s = \\frac{A}{S}$, where $S$ is the total area of the surface of a sample and $A$ is its activity."^^xsd:string ;
  qudt:symbol "a_s"^^xsd:string ;
  rdfs:label "Surface Activity Density"^^xsd:string ;
  skos:prefLabel "Surface Activity Density"^^xsd:string ;
  prov:wasDerivedFrom <http://www.iso.org/iso/catalogue_detail?csnumber=31895> ;
.
quantity:ThermalUtilizationFactor
  rdf:type qudt:AtomicPhysicsQuantityKind ;
  qudt:applicableUnit unit:UNITLESS ;
  qudt:description "The \\textit{Thermal Utilization Factor} in an infinite medium, is the ratio of the number of thermal absorbed in a fissionable nuclide or in a nuclear fuel, as specified, to the total number of thermal neutrons absorbed."^^xsd:string ;
  qudt:symbol "f"^^xsd:string ;
  rdfs:label "Thermal Utilization Factor"^^xsd:string ;
  prov:wasDerivedFrom <http://www.iso.org/iso/catalogue_detail?csnumber=31895> ;
  prov:wasInfluencedBy <http://en.wikipedia.org/wiki/Four_factor_formula> ;
.
quantity:TotalAngularMomentum
  rdf:type qudt:AtomicPhysicsQuantityKind ;
  rdf:type qudt:QuantityKind ;
  qudt:applicableUnit unit:J-SEC ;
  qudt:description "\\textit{Total Angular Momentum} combines both the spin and orbital angular momentum of all particles and fields. In atomic and nuclear physics, orbital angular momentum is usually denoted by $l$ or $L$ instead of $\\Lambda$. The magnitude of $J$ is quantized so that $J^2 = \\hbar^2 j(j + 1)$, where $j$ is the total angular momentum quantum number."^^xsd:string ;
  qudt:symbol "J"^^xsd:string ;
  rdfs:label "Total Angular Momentum"^^xsd:string ;
  skos:broader quantity:AngularMomentum ;
  prov:wasDerivedFrom <http://www.iso.org/iso/catalogue_detail?csnumber=31895> ;
  prov:wasInfluencedBy <http://en.wikipedia.org/wiki/Angular_momentum#Spin.2C_orbital.2C_and_total_angular_momentum> ;
.
quantity:TotalAngularMomentumQuantumNumber
  rdf:type qudt:AtomicPhysicsQuantityKind ;
  rdf:type qudt:QuantityKind ;
  qudt:applicableUnit unit:UNITLESS ;
  qudt:description "The \\textit{Total Angular Quantum Number} describes the magnitude of total angular momentum $J$, where $j$ refers to a specific particle and $J$ is used for the whole system."^^xsd:string ;
  qudt:symbol "j"^^xsd:string ;
  rdfs:label "Total Angular Momentum Quantum Number"^^xsd:string ;
  skos:broader quantity:QuantumNumber ;
  skos:closeMatch quantity:MagneticQuantumNumber ;
  skos:closeMatch quantity:PrincipalQuantumNumber ;
  skos:closeMatch quantity:SpinQuantumNumber ;
  skos:prefLabel "Total Angular Momentum Quantum Number"^^xsd:string ;
  prov:wasDerivedFrom <http://www.iso.org/iso/catalogue_detail?csnumber=31894> ;
  prov:wasInfluencedBy <http://en.wikipedia.org/wiki/Quantum_number> ;
.
quantity:TotalAtomicStoppingPower
  rdf:type qudt:AtomicPhysicsQuantityKind ;
  rdf:type qudt:QuantityKind ;
  qudt:applicableUnit unit:J-M2 ;
  qudt:description "The \\textit{Total Atomic Stopping Power} for an ionizing particle passing through an element, is the particle's energy loss per atom within a unit area normal to the particle's path; equal to the linear energy transfer (energy loss per unit path length) divided by the number of atoms per unit volume."^^xsd:string ;
  qudt:latexDefinition "$S_a = \\frac{S}{n}$, where $S$ is the total linear stopping power and $n$ is the number density of the atoms in the substance."^^xsd:string ;
  qudt:symbol "S_a"^^xsd:string ;
  rdfs:label "Total Atomic Stopping Power"^^xsd:string ;
  skos:prefLabel "Total Atomic Stopping Power"^^xsd:string ;
  prov:wasDerivedFrom <http://www.iso.org/iso/catalogue_detail?csnumber=31895> ;
  prov:wasInfluencedBy <http://www.answers.com/topic/atomic-stopping-power> ;
.
quantity:TotalCross-Section
  rdf:type qudt:AtomicPhysicsQuantityKind ;
  rdf:type qudt:QuantityKind ;
  qudt:applicableUnit unit:M2 ;
  qudt:description "\\textit{Total Cross-section} is related to the absorbance of the light intensity through Beer-Lambert's law. It is the sum of all cross-sections corresponding to the various reactions or processes between an incident particle of specified type and energy and a target particle."^^xsd:string ;
  qudt:symbol "\\sigma_{tot}, \\sigma_T"^^xsd:string ;
  rdfs:label "Total Cross-section"^^xsd:string ;
  skos:broader quantity:Cross-Section ;
  skos:prefLabel "Total Cross-section"^^xsd:string ;
  prov:wasDerivedFrom <http://www.iso.org/iso/catalogue_detail?csnumber=31895> ;
  prov:wasInfluencedBy <http://en.wikipedia.org/wiki/Cross_section_(physics)> ;
.
quantity:TotalIonization
  rdf:type qudt:AtomicPhysicsQuantityKind ;
  rdf:type qudt:QuantityKind ;
  qudt:applicableUnit unit:UNITLESS ;
  qudt:description "\\textit{Total Ionization} by a particle, total mean charge, divided by the elementary charge, $e$, of all positive ions produced by an ionizing charged particle along its entire path and along the paths of any secondary charged particles."^^xsd:string ;
  qudt:latexDefinition "$N = \\int N_i dl$."^^xsd:string ;
  qudt:symbol "N_i"^^xsd:string ;
  rdfs:label "Total Ionization"^^xsd:string ;
  skos:prefLabel "Total Ionization"^^xsd:string ;
  prov:wasDerivedFrom <http://www.iso.org/iso/catalogue_detail?csnumber=31895> ;
  prov:wasInfluencedBy <http://en.wikipedia.org/wiki/Ionization#Classical_ionization> ;
.
quantity:TotalLinearStoppingPower
  rdf:type qudt:AtomicPhysicsQuantityKind ;
  rdf:type qudt:QuantityKind ;
  qudt:applicableUnit unit:J-PER-M ;
  qudt:description "The \\textit{Total Linear Stopping Power} is defined as the average energy loss of the particle per unit path length."^^xsd:string ;
  qudt:latexDefinition "$S = -\\frac{dE}{dx}$, where $-dE$ is the energy decrement in the $x-direction$ along an elementary path with the length $dx$."^^xsd:string ;
  qudt:symbol "S"^^xsd:string ;
  rdfs:label "Total Linear Stopping Power"^^xsd:string ;
  skos:prefLabel "Total Linear Stopping Power"^^xsd:string ;
  prov:wasDerivedFrom <http://www.iso.org/iso/catalogue_detail?csnumber=31895> ;
  prov:wasInfluencedBy <http://en.wikipedia.org/wiki/Stopping_power_(particle_radiation)> ;
.
quantity:TotalMassStoppingPower
  rdf:type qudt:AtomicPhysicsQuantityKind ;
  rdf:type qudt:QuantityKind ;
  qudt:applicableUnit unit:J-M2-PER-KG ;
  qudt:description "If a substance is compared in gaseous and solid form, then the linear stopping powers of the two states are very different just because of the different density. One therefore often divides S(E) by the density of the material to obtain the \\textit{Mass Stopping Power}. The mass stopping power then depends only very little on the density of the material."^^xsd:string ;
  qudt:latexDefinition "$S_m = \\frac{S}{\\rho}$, where $S$ is the total linear stopping power and $\\rho$ is the mass density of the sample."^^xsd:string ;
  qudt:symbol "S_m"^^xsd:string ;
  rdfs:label "Total Mass Stopping Power"^^xsd:string ;
  skos:prefLabel "Total Mass Stopping Power"^^xsd:string ;
  prov:wasDerivedFrom <http://www.iso.org/iso/catalogue_detail?csnumber=31895> ;
  prov:wasInfluencedBy <http://en.wikipedia.org/wiki/Stopping_power_(particle_radiation)> ;
.
