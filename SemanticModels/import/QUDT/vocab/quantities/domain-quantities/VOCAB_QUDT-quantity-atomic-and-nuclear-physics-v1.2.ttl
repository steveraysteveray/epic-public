  qudt:applicableUnit unit:UNITLESS ;
  qudt:description "The \\textit{g-Factor of Nucleus} is associated with the spin and magnetic moments of protons, neutrons, and many nuclei."^^xsd:string ;
  qudt:latexDefinition "$g = \\frac{\\mu}{I\\mu_B}$, where $\\mu$ is the magnitude of magnetic dipole moment, $I$ is the nuclear angular momentum quantum number, and $\\mu_B$ is the Bohr magneton."^^xsd:string ;
