quantity:ParticleSourceDensity
  rdf:type qudt:AtomicPhysicsQuantityKind ;
  rdf:type qudt:QuantityKind ;
  qudt:applicableUnit unit:PER-M3-SEC ;
  qudt:description "\\textit{Particle Source Density} is the rate of production of particles in a 3D domain divided by the volume of that element."^^xsd:string ;
