quantity:FineStructureConstant
  rdf:type qudt:AtomicPhysicsQuantityKind ;
  qudt:applicableUnit unit:UNITLESS ;
  qudt:description "The \\textit{Fine-structure Constant}  is a fundamental physical constant, namely the coupling constant characterizing the strength of the electromagnetic interaction."^^xsd:string ;
  qudt:latexDefinition "$a = \\frac{e^2}{4\\pi\\varepsilon_0\\hbar c_0}$, where $e$ is the elementary charge, $\\varepsilon_0$ is the electric constant, item $\\hbar$ is the reduced Planck constant, and $c_0$ is the speed of light in vacuum."^^xsd:string ;
