quantity:GyromagneticRatio
  rdf:type qudt:AtomicPhysicsQuantityKind ;
  rdf:type qudt:PhysicalConstant ;
  qudt:applicableUnit unit:A-M2-PER-J-SEC ;
  qudt:description "\\textit{Gyromagnetic Ratio}, also sometimes known as the magnetogyric ratio in other disciplines, of a particle or system is the ratio of its magnetic dipole moment to its angular momentum, and it is often denoted by the symbol, $\\gamma$. Its SI units are radian per second per tesla ($rad s^{-1} \\cdot T^{1}$) or, equivalently, coulomb per kilogram ($C \\cdot kg^{-1}$)."^^xsd:string ;
  qudt:latexDefinition "$\\mu = \\gamma J$, where $\\mu$ is the magnetic dipole moment, and $J$ is the total angular momentum."^^xsd:string ;
  qudt:quantityValue quantity:Value_ElectronGyromagneticRatio ;
  qudt:symbol "\\gamma"^^xsd:string ;
  voag:hasGovernance quantity:CODATA_GovernanceDetails ;
  rdfs:label "Gyromagnetic Ratio"^^xsd:string ;
  skos:broader quantity:ElectricChargePerMass ;
  skos:closeMatch <http://dbpedia.org/resource/Gyromagnetic_ratio> ;
  skos:exactMatch <http://dbpedia.org/resource/Gyromagnetic_ratio> ;
  skos:prefLabel "Gyromagnetic Ratio"^^xsd:string ;
  prov:wasInfluencedBy <http://en.wikipedia.org/wiki/Gyromagnetic_ratio> ;
.
