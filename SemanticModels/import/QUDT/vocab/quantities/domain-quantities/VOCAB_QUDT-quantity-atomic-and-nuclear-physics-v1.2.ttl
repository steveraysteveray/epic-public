quantity:ParticleCurrent
  rdf:type qudt:AtomicPhysicsQuantityKind ;
  rdf:type qudt:QuantityKind ;
  qudt:applicableUnit unit:PER-M2-SEC ;
  qudt:description "\\textit{Particle Current} can be used to describe the net number of particles passing through a surface in an infinitesimal time interval."^^xsd:string ;
  qudt:latexDefinition "$\\int J \\cdot e_n dA = \\frac{dN}{dt}$, where $e_ndA$ is the vector surface element, $N$ is the net number of particles passing over a surface, and $dt$ describes the time interval."^^xsd:string ;
  qudt:symbol "J, S"^^xsd:string ;
  rdfs:label "Particle Current"^^xsd:string ;
  skos:prefLabel "Particle Current"^^xsd:string ;
  prov:wasDerivedFrom <http://www.iso.org/iso/catalogue_detail?csnumber=31895> ;
.
quantity:ParticleFluence
  rdf:type qudt:AtomicPhysicsQuantityKind ;
  rdf:type qudt:QuantityKind ;
  qudt:applicableUnit unit:PER-M2 ;
  qudt:description "\\textit{Particle Fluence} is the total number of particles that intersect a unit area in a specific time interval of interest, and has units of m–2 (number of particles per meter squared)."^^xsd:string ;
  qudt:latexDefinition "$\\Phi = \\frac{dN}{dA}$, where $dN$ describes the number of particles incident on a small spherical domain at a given point in space, and $dA$ describes the cross-sectional area of that domain."^^xsd:string ;
  qudt:symbol "\\Phi"^^xsd:string ;
  rdfs:label "Particle Fluence"^^xsd:string ;
  skos:prefLabel "Particle Fluence"^^xsd:string ;
  prov:wasDerivedFrom <http://www.iso.org/iso/catalogue_detail?csnumber=31895> ;
  prov:wasInfluencedBy <http://en.wikipedia.org/wiki/Fluence> ;
.
quantity:ParticleFluenceRate
  rdf:type qudt:AtomicPhysicsQuantityKind ;
  rdf:type qudt:QuantityKind ;
  qudt:applicableUnit unit:PER-M2-SEC ;
  qudt:description "\\textit{Particle Fluence Rate} can be defined as the total number of particles (typically Gamma Ray Photons ) crossing over a sphere of unit cross section which surrounds a Point Source of Ionising Radiation."^^xsd:string ;
  qudt:latexDefinition "$\\theta = \\frac{d\\Phi}{dt}$, where $d\\Phi$ is the increment of the particle fluence during an infinitesimal time interval with duration $dt$."^^xsd:string ;
  qudt:symbol "\\theta"^^xsd:string ;
  rdfs:label "Particle Fluence Rate"^^xsd:string ;
  skos:prefLabel "Particle Fluence Rate"^^xsd:string ;
  prov:wasDerivedFrom <http://www.iso.org/iso/catalogue_detail?csnumber=31895> ;
  prov:wasInfluencedBy <http://www.encyclo.co.uk/define/Fluence%20Rate> ;
.
quantity:ParticleNumberDensity
  rdf:type qudt:AtomicPhysicsQuantityKind ;
  rdf:type qudt:QuantityKind ;
  qudt:applicableUnit unit:PER-M3 ;
  qudt:description "The \\textit{Particle Number Density} is obtained by dividing the particle number of a system by its volume."^^xsd:string ;
  qudt:latexDefinition "$n = \\frac{N}{V}$, where $N$ is the number of particles in the 3D domain with the volume $V$."^^xsd:string ;
