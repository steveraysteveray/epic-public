quantity:Kerma
  rdf:type qudt:AtomicPhysicsQuantityKind ;
  rdf:type qudt:QuantityKind ;
  qudt:applicableUnit unit:GRAY ;
  qudt:description "\\textit{Kerma} is the sum of the initial kinetic energies of all the charged particles liberated by uncharged ionizing radiation (i.e., indirectly ionizing radiation such as photons and neutrons) in a sample of matter, divided by the mass of the sample."^^xsd:string ;
  qudt:latexDefinition "For indirectly ionizing (uncharged) particles, $K= \\frac{dE_{tr}}{dm}$, where $dE_{tr}$ is the mean sum of the initial kinetic energies of all the charged ionizing particles liberated by uncharged ionizing particles in an element of matter, and $dm$ is the mass of that element."^^xsd:string ;
  qudt:symbol "K"^^xsd:string ;
  rdfs:label "Kerma"^^xsd:string ;
  prov:wasDerivedFrom <http://www.iso.org/iso/catalogue_detail?csnumber=31895> ;
  prov:wasInfluencedBy <http://en.wikipedia.org/wiki/Kerma_(physics)> ;
.
