  qudt:latexDefinition "$g = \\frac{\\mu}{J\\mu_B}$, where $\\mu$ is the magnitude of magnetic dipole moment, $J$ is the total angular momentum quantum number, and $\\mu_B$ is the Bohr magneton."^^xsd:string ;
  qudt:symbol "g"^^xsd:string ;
  rdfs:label "Lande g-Factor"^^xsd:string ;
  skos:prefLabel "Lande g-Factor"^^xsd:string ;
