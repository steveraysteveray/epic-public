.
quantity:Reactivity
  rdf:type qudt:AtomicPhysicsQuantityKind ;
  qudt:applicableUnit unit:UNITLESS ;
  qudt:description "\\textit{Reactivity} characterizes the deflection of reactor from the critical state."^^xsd:string ;
  qudt:latexDefinition "$\\rho = \\frac{k_{eff} - 1}{k_{eff}}$, where $k_{eff}$ is the effective multiplication factor."^^xsd:string ;
  qudt:symbol "\\rho"^^xsd:string ;
  rdfs:label "Reactivity"^^xsd:string ;
  prov:wasDerivedFrom <http://www.iso.org/iso/catalogue_detail?csnumber=31895> ;
  prov:wasInfluencedBy <http://en.wikipedia.org/wiki/Nuclear_chain_reaction> ;
.
quantity:ReactorTimeConstant
  rdf:type qudt:AtomicPhysicsQuantityKind ;
  qudt:applicableUnit unit:SECOND ;
  qudt:description "The \\textit{Reactor Time Constant}, also called the reactor period, is the time during which the neutron flux density in a reactor changes by the factor e = 2.718 (e: basis of natural logarithms), when the neutron flux density increases or decreases exponentially."^^xsd:string ;
  qudt:symbol "T"^^xsd:string ;
  rdfs:label "Reactor Time Constant"^^xsd:string ;
  prov:wasDerivedFrom <http://www.iso.org/iso/catalogue_detail?csnumber=31895> ;
