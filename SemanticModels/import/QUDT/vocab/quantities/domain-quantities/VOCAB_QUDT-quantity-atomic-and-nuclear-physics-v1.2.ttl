.
quantity:RadiantEnergy
  rdf:type qudt:AtomicPhysicsQuantityKind ;
  rdf:type qudt:QuantityKind ;
  qudt:applicableUnit unit:JOULE ;
  qudt:description "In radiometry,\\textit{Radiant Energy} is the energy of electromagnetic waves. The quantity of radiant energy may be calculated by integrating radiant flux (or power) with respect to time.  In nuclear physics, \\textit{Radiant Energy} is energy, excluding rest energy, of the particles that are emitted, transferred, or received."^^xsd:string ;
  qudt:symbol "R"^^xsd:string ;
  rdfs:label "Radiant Energy"^^xsd:string ;
  skos:prefLabel "Radiant Energy"^^xsd:string ;
  prov:wasDerivedFrom <http://www.iso.org/iso/catalogue_detail?csnumber=31895> ;
  prov:wasInfluencedBy <http://en.wikipedia.org/wiki/Radiant_energy> ;
.
quantity:ReactionEnergy
  rdf:type qudt:AtomicPhysicsQuantityKind ;
  rdf:type qudt:QuantityKind ;
  qudt:applicableUnit unit:JOULE ;
  qudt:description "\\textit{Reaction Energy} in a nuclear reaction, is the sum of the kinetic energies and photon energies of the reaction products minus the sum of the kinetic and photon energies of the reactants."^^xsd:string ;
  qudt:symbol "Q"^^xsd:string ;
  rdfs:label "Reaction Energy"^^xsd:string ;
  skos:prefLabel "Reaction Energy"^^xsd:string ;
  prov:wasDerivedFrom <http://www.iso.org/iso/catalogue_detail?csnumber=31895> ;
