quantity:PlanckConstant
  rdf:type qudt:AtomicPhysicsQuantityKind ;
  rdf:type qudt:PhysicalConstant ;
  qudt:applicableUnit unit:J-SEC ;
  qudt:description "The \\textit{Planck Constant} is a physical constant that is the quantum of action in quantum mechanics. The Planck constant was first described as the proportionality constant between the energy ($E$) of a photon and the frequency ($\\nu$) of its associated electromagnetic wave."^^xsd:string ;
  qudt:latexDefinition "$E = h\\nu = \\frac{hc}{\\lambda}$, where $E$ is energy, $\\nu$ is frequency, $\\lambda$ is wavelength, and $c$ is the speed of light."^^xsd:string ;
  qudt:quantityValue quantity:Value_PlanckConstant ;
  qudt:symbol "h"^^xsd:string ;
  voag:hasGovernance quantity:CODATA_GovernanceDetails ;
  rdfs:label "Planck Constant"^^xsd:string ;
  skos:closeMatch <http://dbpedia.org/resource/Planck_constant> ;
  prov:wasDerivedFrom <http://www.iso.org/iso/catalogue_detail?csnumber=31895> ;
  prov:wasInfluencedBy <http://en.wikipedia.org/wiki/Planck_constant> ;
.
quantity:PrincipalQuantumNumber
  rdf:type qudt:AtomicPhysicsQuantityKind ;
  rdf:type qudt:QuantityKind ;
  qudt:applicableUnit unit:UNITLESS ;
  qudt:description "The \\textit{Principal Quantum Number} describes the electron shell, or energy level, of an atom. The value of $n$ ranges from 1 to the shell containing the outermost electron of that atom."^^xsd:string ;
  qudt:symbol "n"^^xsd:string ;
  rdfs:label "Principal Quantum Number"^^xsd:string ;
