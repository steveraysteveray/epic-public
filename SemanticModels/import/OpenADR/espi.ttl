espi:UsagePoint
  rdf:type owl:Class ;
  dc:description "Logical point on a network at which consumption or production is either physically measured (e.g., metered) or estimated (e.g., unmetered street lights)."^^xsd:string ;
  sxml:tag "http://naesb.org/espi#UsagePoint" ;
  rdfs:label "Usage point"^^xsd:string ;
  rdfs:subClassOf espi:IdentifiedObject ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      dc:description "Specifies the current status of this usage point. Valid values include: 0 = off 1 = on"^^xsd:string ;
      sxml:isAttribute "false"^^xsd:boolean ;
      sxml:order "2"^^xsd:int ;
      rdfs:comment "The actual range for this restriction is espi:UInt8Datatype. However, since \"Use only XSD datatypes for datatype properties\" flag was set, the generated range is xsd:unsignedByte."^^xsd:string ;
      owl:allValuesFrom xsd:unsignedByte ;
      owl:onProperty espi:status ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      dc:description "Specifies the roles that this usage point has been assigned. Bit 1 - isMirror Bit 2 - isPremisesAggregationPoint Bit 3 - isPEV Bit 4 - isDER Bit 5 - isRevenueQuality Bit 6 - isDC Bit 7-16 - Reserved"^^xsd:string ;
      sxml:isAttribute "false"^^xsd:boolean ;
      sxml:order "0"^^xsd:int ;
      rdfs:comment "The actual range for this restriction is espi:HexBinary16Datatype. However, since \"Use only XSD datatypes for datatype properties\" flag was set, the generated range is xsd:hexBinary."^^xsd:string ;
      owl:allValuesFrom xsd:hexBinary ;
      owl:onProperty espi:roleFlags ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      dc:description "[extension] Contains service delivery point information about the UsagePoint if it does represent the service delivery point."^^xsd:string ;
      sxml:isAttribute "false"^^xsd:boolean ;
      sxml:order "3"^^xsd:int ;
      owl:allValuesFrom espi:ServiceDeliveryPoint ;
      owl:onProperty espi:serviceDeliveryPointRef ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      sxml:isAttribute "false"^^xsd:boolean ;
      sxml:order "1"^^xsd:int ;
      owl:allValuesFrom espi:ServiceCategory ;
      owl:onProperty espi:serviceCategoryRef ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:maxCardinality "1"^^xsd:int ;
      owl:onProperty espi:roleFlags ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:maxCardinality "1"^^xsd:int ;
      owl:onProperty espi:serviceCategoryRef ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:maxCardinality "1"^^xsd:int ;
      owl:onProperty espi:serviceDeliveryPointRef ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:maxCardinality "1"^^xsd:int ;
      owl:onProperty espi:status ;
    ] ;
