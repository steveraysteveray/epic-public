espi:TimeConfiguration
  rdf:type owl:Class ;
  dc:description "[extension] Contains attributes related to the configuration of the time service."^^xsd:string ;
  sxml:tag "http://naesb.org/espi#TimeConfiguration" ;
  rdfs:label "Time configuration"^^xsd:string ;
  rdfs:subClassOf espi:IdentifiedObject ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      dc:description "Daylight savings time offset from local standard time."^^xsd:string ;
      sxml:isAttribute "false"^^xsd:boolean ;
      sxml:order "1"^^xsd:int ;
      rdfs:comment "The actual range for this restriction is espi:TimeTypeDatatype. However, since \"Use only XSD datatypes for datatype properties\" flag was set, the generated range is xsd:long."^^xsd:string ;
      owl:allValuesFrom xsd:long ;
      owl:onProperty espi:dstOffset ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      dc:description "Local time zone offset from UTCTime. Does not include any daylight savings time offsets."^^xsd:string ;
      sxml:isAttribute "false"^^xsd:boolean ;
      sxml:order "3"^^xsd:int ;
      rdfs:comment "The actual range for this restriction is espi:TimeTypeDatatype. However, since \"Use only XSD datatypes for datatype properties\" flag was set, the generated range is xsd:long."^^xsd:string ;
      owl:allValuesFrom xsd:long ;
      owl:onProperty espi:tzOffset ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      dc:description "Rule to calculate end of daylight savings time in the current year.  Result of dstEndRule must be greater than result of dstStartRule."^^xsd:string ;
      sxml:isAttribute "false"^^xsd:boolean ;
      sxml:order "0"^^xsd:int ;
      rdfs:comment "The actual range for this restriction is espi:DstRuleTypeDatatype. However, since \"Use only XSD datatypes for datatype properties\" flag was set, the generated range is xsd:hexBinary."^^xsd:string ;
      owl:allValuesFrom xsd:hexBinary ;
      owl:onProperty espi:dstEndRule ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      dc:description "Rule to calculate start of daylight savings time in the current year. Result of dstEndRule must be greater than result of dstStartRule."^^xsd:string ;
      sxml:isAttribute "false"^^xsd:boolean ;
      sxml:order "2"^^xsd:int ;
      rdfs:comment "The actual range for this restriction is espi:DstRuleTypeDatatype. However, since \"Use only XSD datatypes for datatype properties\" flag was set, the generated range is xsd:hexBinary."^^xsd:string ;
      owl:allValuesFrom xsd:hexBinary ;
      owl:onProperty espi:dstStartRule ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "1"^^xsd:int ;
      owl:onProperty espi:dstEndRule ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "1"^^xsd:int ;
      owl:onProperty espi:dstOffset ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "1"^^xsd:int ;
      owl:onProperty espi:dstStartRule ;
    ] ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:cardinality "1"^^xsd:int ;
      owl:onProperty espi:tzOffset ;
    ] ;
