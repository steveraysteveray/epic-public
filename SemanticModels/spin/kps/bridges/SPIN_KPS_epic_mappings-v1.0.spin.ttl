              ] ;
          ]
        ) ;
    ] ;
  spin:constraint [
      rdf:type spl:Argument ;
      spl:predicate sp:arg1 ;
      spl:valueType rdfs:Resource ;
    ] ;
  rdfs:comment """Executes a given BRICK to EPIC_Bldg mapping and binds its result rows to the variables specified on the right hand side.

Example:

    brick:Building mappings_spin:brickToEPICBldgMappingProperty ?opar_class

Testing:

SELECT ?result
WHERE
{
    brick:Building mappings_spin:brickToEPICBldgMappingProperty ?result .
}

Test with brick:Building""" ;
